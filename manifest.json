{
  "manifest_version": 3,
  "name": "Cluber for Chrome",
  "version": "1.0",
  "permissions": ["storage", "alarms"],
  "host_permissions": [
    "https://www.dis.sc.kr/cf_calendar/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Cluber for Chrome"
  },
  "icons": {
    "1024": "1024.png"
  }
}
