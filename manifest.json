{
    "manifest_version": 3,
    "name": "Cluber for Chrome",
    "version": "1.0",
    "permissions": ["scripting"],
    "host_permissions": [
      "https://www.googleapis.com/calendar/v3/*",
      "https://www.dis.sc.kr/*"
    ],

    "icons": {
    "16": "iconBlue16.png",
    "48": "iconBlue48.png",
    "128": "iconBlue128.png"
  },
    "action": {
      "default_popup": "popup.html"
    }
  }
  